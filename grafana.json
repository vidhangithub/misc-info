        "current": {},
        "refresh": 1
      },
      {
        "name": "instance",
        "label": "Instance",
        "type": "query",
        "datasource": "${DS_PROMETHEUS}",
        "refresh": 2,
        "includeAll": true,
        "multi": true,
        "query": "label_values(http_server_requests_seconds_count, instance)",
        "current": {
          "selected": true,
          "text": "All",
          "value": ".*"
        },
        "regex": "",
        "sort": 1
      }
    ]
  },
  "time": {
    "from": "now-1h",
    "to": "now"
  },
